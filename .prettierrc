{
  "printWidth": 100,
  "useTabs": false,
  "semi": true,
  "tabWidth": 2,
  "singleQuote": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "always",
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-organize-imports"]
}
